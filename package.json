{
  "name": "@rafern/canvas-ui",
  "version": "4.0.1",
  "description": "A Typescript UI library which uses HTML canvases",
  "main": "./lib/index.esm.js",
  "exports": "./lib/index.esm.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint . --ext .ts",
    "build": "tsc && node esbuild.mjs",
    "dev": "node esbuild.mjs --dev",
    "docs": "typedoc --options typedoc.json",
    "generate_theme": "node generate_theme.mjs",
    "clean": "rimraf lib/*"
  },
  "files": [
    "lib",
    "package.json"
  ],
  "keywords": [
    "ui",
    "typescript",
    "canvas"
  ],
  "author": "rafern",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rafern/canvas-ui.git"
  },
  "devDependencies": {
    "@knodes/typedoc-plugin-pages": "^0.23.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "esbuild": "^0.14.54",
    "eslint": "^8.21.0",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "local-web-server": "^5.2.1",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "tslib": "^2.4.0"
  }
}
